  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "babel-preset-minify": "^0.5.1",
    "jsdoc": "^3.6.10",
    "tui-jsdoc-template": "^1.2.2"
