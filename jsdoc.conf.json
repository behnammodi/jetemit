{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "name": "Jetemit",
    "useCollapsibles": true,
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "encoding": "utf8",
    "package": "package.json",
    "recurse": true,
    "destination": "docs",
    "readme": "README.md",
    "template": "node_modules/tui-jsdoc-template"
  }
}
